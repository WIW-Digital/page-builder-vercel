{
  "version": 2,
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "routes": [
    {
      "src": "/(css|js|img)/(.*)",
      "dest": "/$1/$2"
    },
    {
      "src": "/index.php",
      "dest": "/api/index.php"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php?page=$1"
    }
  ]
}
